        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Coach": "Shep\\Coach\\Facades\\Coach"
            },
            "providers": [
                "Shep\\Coach\\CoachServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
