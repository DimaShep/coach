{
    "name": "shep/coach",
    "description": "My coach",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy",
            "email": "dmitriy.shepelenko@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*"
    },
    "autoload": {
        "files": [
            "src/helpers/helpers.php"
        ],
        "psr-4": {
